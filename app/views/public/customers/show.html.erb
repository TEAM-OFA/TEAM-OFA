


<h5><strong>マイページ</strong></h5>
<%= link_to"編集する", edit_customer_registration_path, class:'btn btn-sm btn-success' %>

<table class="table">
  <tbody>
    <tr>
      <td>氏名</td>
      <td><%= last_name first_name %>
      </td>
    </tr>

    <tr>
      <td>カナ</td>
      <td><%= last_name_kana first_name_kana %>
      </td>
    </tr>

    <tr>
      <td>郵便番号</td>
      <td><%= post_code %></td>
    </tr>

    <tr>
      <td>住所</td>
      <td><%= address %></td>
    </tr>

    <tr>
      <td>電話番号</td>
      <td><%= phone_number %></td>
    </tr>

    <tr>
      <td>メールアドレス</td>
      <td><%= email %></td>
    </tr>


  </tbody>

</table>
